{
    "name": "go-ticket-frontend",
    "private": true,
    "version": "1.0.2",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "npm run lint && vite",
        "typecheck": "tsc",
        "build": "npm run clean && npm run typecheck && vite build",
        "lint": "eslint src ",
        "preview": "vite preview",
        "format": "prettier --write \"src/**/*.{js,ts,vue,jsx,tsx,json,css,scss,md}\"",
        "version:major": "export ENV_ASSIGN='major' && chmod +x ./version-up.sh && ./version-up.sh",
        "version:patch": "export ENV_ASSIGN='patch' && chmod +x ./version-up.sh && ./version-up.sh"
    },
    "keywords": [
        "GoTicket",
        "React",
        "Frontend",
        "Ticketing",
        "Vite"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.9.0",
        "bootstrap": "^5.3.6",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.13",
        "jwt-decode": "^4.0.0",
        "qrcode.react": "^4.2.0",
        "react": "^19.0.0",
        "react-bootstrap": "^2.10.9",
        "react-datepicker": "^8.3.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.56.3",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.5.3",
        "styled-components": "^6.1.18",
        "uuid": "^11.1.0",
        "zod": "^3.25.28"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "@vitejs/plugin-react": "^4.3.4",
        "babel-plugin-react-compiler": "^19.1.0-rc.1",
        "eslint": "^9.26.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.0.0-rc.1",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.1.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "sass": "1.69.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.32.0",
        "vite": "^6.3.5"
    },
    "overrides": {
        "sass": "1.69.0"
    }
}
